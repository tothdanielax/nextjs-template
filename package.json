{
  "name": "nextjs-template",
  "version": "1.0.0",
  "description": "NextJS Template",
  "keywords": [
    "nextjs",
    "template",
    "typescript",
    "tailwindcss",
    "storybook",
    "playwright",
    "vite",
    "pnpm",
    "docker",
    "postgres",
    "drizzle"
  ],
  "bugs": {
    "url": "https://github.com/tothdanielax/nextjs-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tothdanielax/nextjs-template.git"
  },
  "license": "MIT",
  "author": "Daniel Zs. Toth <toth.zs.daniel@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:migrate": "tsx db/migrate.ts",
    "db:migrate-gen": "npx drizzle-kit generate",
    "db:seed": "tsx db/seed.ts",
    "docker:local": "docker compose -f docker-compose.local.yml --env-file .env up -V --remove-orphans --force-recreate --build --wait -d",
    "format": "prettier --write --ignore-unknown .",
    "lint": "SKIP_ENV_VALIDATION=1 next lint --max-warnings=0",
    "start:dev": "next dev --turbo",
    "start:prod": "next start",
    "storybook:dev": "storybook dev -p 6006",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx lint-staged"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
