    "deepmerge": "^4.3.1",
    "framer-motion": "^11.3.24",
    "happy-dom": "^14.12.3",
    "lucide-react": "^0.427.0",
    "next": "^14.2.5",
    "next-intl": "^3.17.2",
    "next-themes": "^0.3.0",
    "postgres": "^3.4.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
